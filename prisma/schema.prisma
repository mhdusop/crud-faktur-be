generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Faktur {
  id             Int      @id @default(autoincrement())
  no_faktur      String   @unique
  nama_barang    String
  quantity       Int
  harga          Int
  nama_pelanggan String   @db.VarChar(30)
  phone          String
  tgl_faktur     String
  createdAt      DateTime @default(now())
}
